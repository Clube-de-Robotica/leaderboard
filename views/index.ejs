 <!DOCTYPE html>
 <html lang="en">
 <head>
     <meta charset="UTF-8">
     <meta name="viewport" content="width=device-width, initial-scale=1.0">
     <title>Leaderboard</title>
     <link rel="stylesheet" href="/leaderboard/styles.css">
 </head>
 <body>
     <div class="carousel-container">
         <button class="carousel-btn left-btn" onclick="prevSlide()">&#10094;</button>
 
         <div class="carousel">
             <% Object.keys(leaderboardData).forEach((sheetName, index) => { %>
                 <div class="carousel-item <% if (index === 0) { %> active <% } %>">
                    <div class="leaderboard-header">
                        <h1><%= sheetName %></h1>
                        <img src="/leaderboard/<%= sheetName %>.png" alt="Logo" class="leaderboard-logo">
                    </div>
                     <table>
                         <thead>
                             <tr>
                                 <% Object.keys(leaderboardData[sheetName][0]).forEach(header => { %>
                                     <th><%= header %></th>
                                 <% }); %>
                             </tr>
                         </thead>
                         <tbody>
                             <% leaderboardData[sheetName].forEach(row => { %>
                                 <tr>
                                     <% Object.values(row).forEach(value => { %>
                                         <td><%= value %></td>
                                     <% }); %>
                                 </tr>
                             <% }); %>
                         </tbody>
                     </table>
                 </div>
             <% }); %>
         </div>
 
         <button class="carousel-btn right-btn" onclick="nextSlide()">&#10095;</button>
     </div>
 
     <script src="/leaderboard/script.js"></script>
 </body>
 </html>
 